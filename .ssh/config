Host *
  ChallengeResponseAuthentication no
  Protocol 2
  PasswordAuthentication no
  AddKeysToAgent ask
  UseKeychain yes

Host heroku-bluespot
  HostName heroku.com
  IdentityFile ~/.ssh/heroku_bluespot_rsa

Host heroku-personal
  HostName heroku.com
  IdentityFile ~/.ssh/heroku_personal_rsa

Host github.com
  IdentityFile ~/.ssh/github_rsa

Host gist.github.com
  IdentityFile ~/.ssh/github_rsa

Host bitbucket.org
  IdentityFile ~/.ssh/bitbucket_rsa

Host medidata
  HostName cl01-mde.flexiion-customer.net
  IdentityFile ~/.ssh/medidata_rsa
  Port 8022
  User bluespot
