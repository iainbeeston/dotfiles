Host *
  ChallengeResponseAuthentication no
  Protocol 2
  PasswordAuthentication no
  AddKeysToAgent yes
  UseKeychain yes

Host heroku-bluespot
  HostName heroku.com
  IdentityFile ~/.ssh/heroku_bluespot_rsa

Host heroku-personal
  HostName heroku.com
  IdentityFile ~/.ssh/heroku_personal_rsa

Host github.com
  IdentityFile ~/.ssh/github_rsa
